<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#26a631" />
        </shape>
    </item>
    <item android:left="3dp" android:right="3dp"  android:top="3dp" android:bottom="3dp" >
        <shape android:shape="rectangle">
            <solid android:color="#8fce9e" />
        </shape>
    </item>
    <item>

        <shape>
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp"    >
            </padding>
            <corners
                android:radius="11dp">
            </corners>
        </shape>
    </item>
</layer-list>